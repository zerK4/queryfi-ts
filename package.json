{
  "name": "queryfi",
  "version": "0.1.0",
  "description": "A flexible TypeScript query builder",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "prepublishOnly": "npm run rebuild",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "keywords": [
    "query-builder",
    "typescript",
    "api"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "zod": "^3.x.x"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^16.x.x",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.11",
    "msw": "^2.6.8",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "typescript": "^4.x.x"
  }
}
